<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="assets_backend" name="MRP Extend Barcode" inherit_id="web.assets_backend">
        <xpath expr="." position="inside">
            <script type="text/javascript" src="/mrp_extended/static/js/barcode_main_menu.js"/>
        </xpath>
        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/mrp_extended/static/js/mrp_barcode_kanban_record.js"/>
            <script type="text/javascript" src="/mrp_extended/static/js/mrp_barcode_kanban_renderer.js"/>
            <script type="text/javascript" src="/mrp_extended/static/js/mrp_barcode_kanban_view.js"/>
        </xpath>
    </template>
</odoo>